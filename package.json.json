    "compression": "^1.7.4",
    "express-rate-limit": "^6.10.0",
    "node-bash-title": "^0.0.1",
    "axios": "^1.6.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.0",
    "jest": "^29.0.0",
    "supertest": "^6.3.0"
  },
  "engines": {
    "node": ">=16.0.0",
